{
  "name": "nest-angular",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:client": "ng build",
    "build:client:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:server": "webpack --progress --colors",
    "build:server:prod": "webpack -p --progress --colors",
    "build": "npm run build:client && npm run build:server",
    "watch:server": "webpack --w --colors",
    "watch:client": "ng serve --proxy-config proxy.conf.json",
    "watch": "concurrent \" npm run watch:server \" \" npm run watch:client \" ",
    "build:universal:browser-and-server": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "build:universal": "npm run build:universal:browser-and-server && npm run build:server",
    "postinstall": "npm run build:universal"
  },
